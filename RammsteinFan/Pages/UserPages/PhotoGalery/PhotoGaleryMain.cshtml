@page
@model RammsteinFan.Pages.UserPages.PhotoGaleryModel
@{
    ViewData["Title"] = "Фотогалерея";
    ViewData["chapter"] = "Фотогалерея";
}
<div class="container-fluid">
    <div class="row justify-content-center">
        <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
            @foreach (var photoAlbum in Model.PhotoGalery)
            {
                <a class="photosBtn row" asp-page="/UserPages/PhotoGalery/ConcretePhotoAlbum" asp-route-title="@photoAlbum">
                    <div class="photoImg col-3">
                        <img src="~/img/photoGalery/@(photoAlbum)/@(photoAlbum).jpg">
                    </div>
                    <p class="col-8">@photoAlbum</p>
                </a>
            }
        </div>
    </div>
</div>